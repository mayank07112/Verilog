#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01323A30 .scope module, "testbranch" "testbranch" 2 2;
 .timescale -9 -12;
v0132EDF8_0 .net "c", 3 0, v01322C30_0; 1 drivers
v0132EE50_0 .var "clk", 0 0;
v0132EEA8_0 .var "r", 0 0;
v0132EF00_0 .var "u_d", 0 0;
S_01322B50 .scope module, "b1" "bcd" 2 6, 3 1, S_01323A30;
 .timescale 0 0;
v01322BD8_0 .net "clk", 0 0, v0132EE50_0; 1 drivers
v01322C30_0 .var "count", 3 0;
v0132ED48_0 .net "reset", 0 0, v0132EEA8_0; 1 drivers
v0132EDA0_0 .net "up_down", 0 0, v0132EF00_0; 1 drivers
E_01329220 .event posedge, v01322BD8_0;
    .scope S_01322B50;
T_0 ;
    %wait E_01329220;
    %load/v 8, v0132ED48_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v01322C30_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0132EDA0_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v01322C30_0, 4;
    %cmpi/u 8, 9, 4;
    %jmp/0xz  T_0.4, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v01322C30_0, 0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/v 8, v01322C30_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v01322C30_0, 0, 8;
T_0.5 ;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v01322C30_0, 4;
    %cmpi/u 8, 0, 4;
    %jmp/0xz  T_0.6, 4;
    %movi 8, 9, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v01322C30_0, 0, 8;
    %jmp T_0.7;
T_0.6 ;
    %load/v 8, v01322C30_0, 4;
    %mov 12, 0, 28;
    %subi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v01322C30_0, 0, 8;
T_0.7 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01323A30;
T_1 ;
    %delay 5000, 0;
    %load/v 8, v0132EE50_0, 1;
    %inv 8, 1;
    %set/v v0132EE50_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_01323A30;
T_2 ;
    %set/v v0132EE50_0, 0, 1;
    %set/v v0132EEA8_0, 1, 1;
    %set/v v0132EF00_0, 1, 1;
    %delay 10000, 0;
    %set/v v0132EEA8_0, 0, 1;
    %delay 100000, 0;
    %set/v v0132EF00_0, 0, 1;
    %delay 100000, 0;
    %set/v v0132EEA8_0, 1, 1;
    %delay 10000, 0;
    %set/v v0132EEA8_0, 0, 1;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_2;
    .scope S_01323A30;
T_3 ;
    %vpi_call 2 26 "$display", "Time\011clk\011rst\011up_down\011count";
    %vpi_call 2 27 "$monitor", "%g\011%b\011%b\011%b\011%b", $time, v0132EE50_0, v0132EEA8_0, v0132EF00_0, v0132EDF8_0;
    %end;
    .thread T_3;
    .scope S_01323A30;
T_4 ;
    %vpi_call 2 30 "$dumpfile", "bcd_counter.vcd";
    %vpi_call 2 31 "$dumpvars", 1'sb0, S_01323A30;
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bcd_up_and_down_counter_tb.v";
    "bcd_up_and_down_counter.v";
