#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01068658 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v01062C40_0 .var "clk", 0 0;
v01063DC8_0 .net "light", 1 0, v01064F00_0; 1 drivers
E_00F422B8 .event edge, v01064F00_0;
S_01064BF8 .scope module, "t1" "traffic_signal" 2 4, 3 1, S_01068658;
 .timescale 0 0;
P_01062B64 .param/l "GREEN" 3 5, C4<01>;
P_01062B78 .param/l "RED" 3 5, C4<00>;
P_01062B8C .param/l "S0" 3 6, +C4<0>;
P_01062BA0 .param/l "S1" 3 6, +C4<01>;
P_01062BB4 .param/l "S2" 3 6, +C4<010>;
P_01062BC8 .param/l "YELLOW" 3 5, C4<10>;
v01064C80_0 .net "clk", 0 0, v01062C40_0; 1 drivers
v01064F00_0 .var "light", 1 0;
v01062BE8_0 .var "state", 0 1;
E_00F42618 .event edge, v01062BE8_0;
E_00F42418 .event posedge, v01064C80_0;
    .scope S_01064BF8;
T_0 ;
    %wait E_00F42418;
    %load/v 8, v01062BE8_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %ix/load 0, 2, 0;
    %assign/v0 v01062BE8_0, 0, 0;
    %jmp T_0.4;
T_0.0 ;
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v01062BE8_0, 0, 8;
    %jmp T_0.4;
T_0.1 ;
    %movi 8, 2, 3;
    %ix/load 0, 2, 0;
    %assign/v0 v01062BE8_0, 0, 8;
    %jmp T_0.4;
T_0.2 ;
    %ix/load 0, 2, 0;
    %assign/v0 v01062BE8_0, 0, 0;
    %jmp T_0.4;
T_0.4 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01064BF8;
T_1 ;
    %wait E_00F42618;
    %load/v 8, v01062BE8_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %ix/load 0, 2, 0;
    %assign/v0 v01064F00_0, 0, 0;
    %jmp T_1.4;
T_1.0 ;
    %ix/load 0, 2, 0;
    %assign/v0 v01064F00_0, 0, 0;
    %jmp T_1.4;
T_1.1 ;
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v01064F00_0, 0, 8;
    %jmp T_1.4;
T_1.2 ;
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v01064F00_0, 0, 8;
    %jmp T_1.4;
T_1.4 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_01068658;
T_2 ;
    %delay 5, 0;
    %load/v 8, v01062C40_0, 1;
    %inv 8, 1;
    %set/v v01062C40_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_01068658;
T_3 ;
    %set/v v01062C40_0, 0, 1;
    %vpi_call 2 9 "$monitor", $time, "light=%b", v01063DC8_0;
    %delay 100, 0;
    %vpi_call 2 10 "$finish";
    %end;
    .thread T_3;
    .scope S_01068658;
T_4 ;
    %wait E_00F422B8;
    %load/v 8, v01063DC8_0, 2;
    %mov 10, 0, 1;
    %cmpi/u 8, 0, 3;
    %jmp/0xz  T_4.0, 4;
    %vpi_call 2 16 "$display", $time, "RED";
    %jmp T_4.1;
T_4.0 ;
    %load/v 8, v01063DC8_0, 2;
    %mov 10, 0, 1;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_4.2, 4;
    %vpi_call 2 20 "$display", $time, "GREEN";
    %jmp T_4.3;
T_4.2 ;
    %vpi_call 2 24 "$display", $time, "YELLOW";
T_4.3 ;
T_4.1 ;
    %jmp T_4;
    .thread T_4, $push;
# The file index is used to find the file name in the following table.
:file_names 4;
